{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"illuminate/html": "~5.0",
	"require": {
		"laravel/framework": "5.0.*",
		"maatwebsite/excel": "~2.0.0",
		"geoip2/geoip2": "~2.0",
		"maxmind-db/reader": "~1.0",
		"videlalvaro/php-amqplib": "2.2.*",
        "googleads/googleads-php-lib": "^40.0",
		"braintree/braintree_php" : "3.7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"/opt/php70/bin/php artisan clear-compiled",
			"/opt/php70/bin/php artisan optimize"
		],
		"post-update-cmd": [
			"/opt/php70/bin/php artisan clear-compiled",
			"/opt/php70/bin/php artisan optimize"
		],
		"post-create-project-cmd": [
			"/opt/php70/bin/php -r \"copy('.env.example', '.env');\"",
			"/opt/php70/bin/php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist",
		"minimum-stability": "dev",
    "prefer-stable": true
	},
	"minimum-stability": "dev",
    "prefer-stable": true
}
